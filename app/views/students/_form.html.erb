<%= form_for student do |f| %>
<table>
  <tr>
    <td><strong><%= f.label :name %></strong></td>
    <td><%= f.text_field :name %></td>
  </tr>
  <tr>
    <td><strong><%= f.label :email %></strong></td>
    <td><%= f.email_field :email %></td>
  </tr>
</table>
<strong>
  <%= link_to "Cancel", students_path, class: "btn btn-sm btn-secondary" %>
</strong>
<%= f.submit %>
<% end %>
